{
  "name": "wmb",
  "version": "1.0.0",
  "description": "building static",
  "main": "js/main.js",
  "dependencies": {
    "babel": "^6.3.26",
    "babel-core": "^6.4.5",
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.22.0",
    "css-loader": "^0.21.0",
    "es5-shim": "^4.4.0",
    "es6-promise": "^3.0.2",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^3.1.0",
    "eslint-plugin-react": "^3.15.0",
    "exports-loader": "^0.6.2",
    "imports-loader": "^0.6.5",
    "node-libs-browser": "^0.5.2",
    "node-sass": "^3.7.0",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.0",
    "webpack": "^2.2.1",
    "whatwg-fetch": "^0.10.1"
  },
  "devDependencies": {
    "better-npm-run": "0.0.14",
    "eslint": "^1.10.3",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.2",
    "eslint-plugin-standard": "^2.0.1"
  },
  "scripts": {
    "build:dist": "better-npm-run build:dist",
    "build:prod": "better-npm-run build:prod",
    "build:dll": "better-npm-run build:dll",
    "test": "better-npm-run test"
  },
  "betterScripts": {
    "build:dist": {
      "command": "webpack --config webpack.dev.config.js --progress --colors",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build:prod": {
      "command": "webpack --config webpack.prod.config.js --progress --colors",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:dll": {
      "command": "webpack --config webpack.dll.config.js --progress --color",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "test": {
      "command": "karma start",
      "env": {
        "NODE_ENV": "test"
      }
    }
  },
  "author": "mcurrr",
  "license": "ISC"
}
