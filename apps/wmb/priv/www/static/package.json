  "betterScripts": {
    "start": {
      "command": "node ./dev-server.js --progress --colors",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build:prod": {
      "command": "webpack --config webpack.config.prod.js --progress --colors",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:dll": {
      "command": "webpack --config webpack.config.dll.js --progress --color",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "test": {
      "command": "karma start",
      "env": {
        "NODE_ENV": "test"
      }
    }
